<%
	int num =0;
	String id= "";
	String contents = "";
	String ip= ""; 
	if(request.getParameter("num")!=null){
		num=MyUtil.parseInt(request, "num");
		id = request.getParameter("id");
		contents= request.getParameter("contents");
		ip = request.getParameter("ip");
	}
%>
